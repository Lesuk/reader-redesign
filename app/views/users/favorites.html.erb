<% provide(:title, @user.name) %>

<%= render 'shared/profile_box' %>

<div class="content" role="main">  
	<ul id="timeline" class="clearfix">

		<% if @favorites.any? %>
		<%= render partial: 'shared/feed_item', collection: @favorites %>
		<% end %>

	</ul>

	<div id="infinite-scrolling">
		<%#= will_paginate @favorites %>
	</div>
</div>