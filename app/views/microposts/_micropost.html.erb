<li id="<%= micropost.id %>">
	<span class="content"><%= auto_link(micropost.content) %></span>
	<span class="timestamp">
		Posted <%= time_ago_in_words(micropost.created_at) %> ago
	</span>
	<% if current_user?(micropost.user) || current_user.admin? %>
    <%= link_to 'Delete', micropost, confirm: 'Are you sure?', method: :delete, remote: true %>
    <p><%= puts micropost.id %></p>
  	<% end %>
</li>